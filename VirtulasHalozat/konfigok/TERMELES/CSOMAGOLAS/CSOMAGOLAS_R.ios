hostname GYARTAS_R

ip access-list extended GUEST-ISOLATE
deny ip 10.4.200.0 0.0.0.255 10.4.0.0 0.0.255.255
permit ip 10.4.200.0 0.0.0.255 any
ip access-list extended ADMIN-PROTECT
permit ip 10.4.10.0 0.0.0.255 10.4.150.0 0.0.0.255
deny ip any 10.4.150.0 0.0.0.255 
permit ip any any
int gig0/0
description TermelesR_Fele
ip address 192.168.3.2 255.255.255.252
ip ospf message-digest-key 2 md5 jelszo
ip ospf authentication message-digest
ip ospf network point-to-point
no sh

int se0/0/0
