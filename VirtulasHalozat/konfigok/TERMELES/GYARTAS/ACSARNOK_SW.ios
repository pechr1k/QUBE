hostname ACSARNOK_SW
ip dhcp snooping
ip dhcp snooping vlan 10,20,30,40,50,60,90,150,200,999
ip arp inspection vlan 10,20,30,40,50,60,90,150,200,999

vlan 999
name NATIVE
vlan 150
name ADMIN
int vlan 150
no sh
ip address 10.3.150.6 255.255.255.0
ip default-gateway 10.3.150.1
vlan 10
name IT
vlan 20
name RECEPTION
vlan 30
name MANAGEMENT
vlan 40
name OFFICE
vlan 50
name PRODUCTION
vlan 60
name WAREHOUSE
vlan 90
name PHONE
vlan 200
name GUEST
exit
spanning-tree mode rapid-pvst
spanning-tree vlan 10,20,30,40,50,60,90,150,200,999 priority 8192

interface range fa0/1-2
description toITSW_ETCH2
channel-group 2 mode passive
interface port-channel 2
ip dhcp snooping trust
ip arp snooping trust
sw mode trunk
switchport trunk native vlan 999
switchport trunk allowed vlan 10,20,30,40,50,60,90,150,200,999
switchport nonegotiate
interface range fa0/3-4
description toACSARNOKKSW_ETCH3
channel-group 3 mode passive
interface port-channel 3
ip dhcp snooping trust
ip arp snooping trust
sw mode trunk
switchport trunk native vlan 999
switchport trunk allowed vlan 10,20,30,40,50,60,90,150,999
switchport nonegotiate

interface range fa0/5-20
description toACCSESS
sw mode acc
sw acc vlan 200
storm-control broadcast level 40.00
spanning-tree bpduguard enable
spanning-tree portfast
dhcp snooping limit rate 5